<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">
  <model id="toy_ode_update" name="ODE metabolite update submodel">
    <listOfCompartments>
      <compartment id="extern" name="external compartment" spatialDimensions="3" size="1" constant="true"/>
      <compartment id="cell" name="cell" spatialDimensions="3" size="1" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="A" name="A" compartment="extern" initialAmount="10" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="C" name="C" compartment="extern" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="B1" name="B1" compartment="cell" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="B2" name="B2" compartment="cell" initialAmount="0" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="v_R1" name="R1 flux" value="0" constant="false"/>
      <parameter id="v_R2" name="R2 flux" value="0" constant="false"/>
      <parameter id="v_R3" name="R3 flux" value="0" constant="false"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="R1" name="A import (R1)" reversible="true" fast="false">
        <listOfReactants>
          <speciesReference species="A" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="B1" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> v_R1 </ci>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R2" name="B1 &lt;-&gt; B2 (R2)" reversible="true" fast="false">
        <listOfReactants>
          <speciesReference species="B1" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="B2" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> v_R2 </ci>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="R3" name="B2 export (R3)" reversible="true" fast="false">
        <listOfReactants>
          <speciesReference species="B2" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="C" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> v_R3 </ci>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
